/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args)
	{
		Scanner sc= new Scanner(System.in);
		
		int T=sc.nextInt();
		
		for(int i=1;i<=T;i++)
		{
		    int X=sc.nextInt();
		    if(X<=100)
		    {
		        int M= 100-X;
    		    System.out.println(""+M);
		    }
		}
	}
}

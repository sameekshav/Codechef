import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) 
	{
		Scanner s= new Scanner(System.in);
		int T= s.nextInt();
		
		for(int i=1;i<=T;i++)
		{
		    int X=s.nextInt();
		    int Y=s.nextInt();
		    
		    int total= (X*10) + (Y*5);
		    System.out.println(""+total);
		}
	}
}
